{
    "version": 2,
    "builds": [
      {
        "src": "nodebb/build/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "nodebb/build/index.js"
      },
      {
        "src": "/public/(.*)",
        "dest": "public/$1"
      }
    ]
  }